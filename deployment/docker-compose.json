{
  "version": "3",
  "services": {
    "db": {
      "image": "postgres:11.5-alpine",
      "ports": [
        "5433:5433"
      ],
      "command": [
        "-p",
        "5433"
      ],
      "environment": {
        "POSTGRES_DB": "banksaurus",
        "POSTGRES_PASSWORD": "banksaurus",
        "POSTGRES_USER": "banksaurus"
      }
    },
    "service": {
      "image": "banksaurus",
      "ports": ["8081:8080"],
      "depends_on": ["db"]
    }
  }
}
